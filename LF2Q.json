{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
   
   
      "title": {
      "text": "Unemployment Rate",
      "subtitle": "   %  aged 16-64, Seasonally Adjusted, Source:ONS",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start"
     
    },
   
   
   
   
   
   
    "data": {
